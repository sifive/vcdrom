{
  "name": "vcdrom",
  "version": "0.7.0",
  "description": "Standalone VCD viewer PWA application",
  "main": "index.js",
  "scripts": {
    "test": "eslint lib",
    "browserify": "browserify ./lib/vcdrom.js > app/vcdrom.js",
    "build": "browserify ./lib/vcdrom.js | terser --compress -o app/vcdrom.js",
    "watch": "watchify ./lib/vcdrom.js -o app/vcdrom.js -v"
  },
  "files": [
    "app/*.html",
    "app/*.ico",
    "app/*.woff2",
    "app/*.wasm",
    "app/*.css",
    "app/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavedrom/vcdrom.git"
  },
  "author": "alex.drom@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wavedrom/vcdrom/issues"
  },
  "homepage": "https://github.com/wavedrom/vcdrom#readme",
  "devDependencies": {
    "browserify": "^17.0.0",
    "codemirror": "^5.65.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "event-debug": "^2.1.0",
    "http-server": "^14.1.0",
    "lodash.get": "^4.4.2",
    "onml": "^2.1.0",
    "pubnub": "^4.24.4",
    "terser": "^5.10.0",
    "vcd-stream": "^1.3.0",
    "watchify": "^4.0.0",
    "waveql": "^0.4.1",
    "websocket": "^1.0.29",
    "ws": "8.6.0"
  },
  "eslintConfig": {
    "extends": "@drom/eslint-config/eslint4/node8",
    "env": {
      "es2020": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "dependencies": {
    "pck": "^0.0.1",
    "pkg": "^5.7.0"
  }
}
